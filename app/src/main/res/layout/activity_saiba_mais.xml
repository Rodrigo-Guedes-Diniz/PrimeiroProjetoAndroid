<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SaibaMais">

    <TextView
        android:id="@+id/LogoSaibaMais"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OurStreet"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.063"
        android:fontFamily="sans-serif-black"
        android:textSize="30dp"/>

    <TextView
        android:id="@+id/texto"
        android:layout_width="375dp"
        android:layout_height="381dp"
        android:text="@string/Texto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/LogoSaibaMais"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/LogoSaibaMais"
        app:layout_constraintTop_toBottomOf="@+id/LogoSaibaMais"
        app:layout_constraintVertical_bias="0.251" />

    <Button
        android:id="@+id/BotaoVoltar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/BotaoVoltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/texto"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/texto"
        app:layout_constraintTop_toBottomOf="@+id/texto"
        app:layout_constraintVertical_bias="0.35" />

</androidx.constraintlayout.widget.ConstraintLayout>